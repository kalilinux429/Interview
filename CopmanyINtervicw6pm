
const data = [
  { Amount: "4.77", ServiceType: "Service1" },
  { Amount: "4.77", ServiceType: "Service1" },
  { Amount: "4.77", ServiceType: "Service2" },
];

const totalAmount = data
  .filter(item => item.ServiceType === "Service1")
  .reduce((sum, item) => sum + parseFloat(item.Amount), 0);

console.log(totalAmount.toFixed(2)); // "9.54"
